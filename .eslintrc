{
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier"],
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js", ".jsx"]
      }
    ],
    "react/prop-types": 0,
    "no-underscore-dangle": 0,
    "semi": [1, "always"],
    "object-curly-spacing": ["error", "always", { "objectsInObjects": false }],
    "import/imports-first": ["error", "absolute-first"]
  },
  "parser": "babel-eslint",
  "env": {
    "jasmine": true
  }
}
